    "panel.border": "#b84777",
    "sash.hoverBorder": "#b84777",
    "statusBar.background": "#93395f",
    "statusBar.foreground": "#e7e7e7",
    "statusBarItem.hoverBackground": "#b84777",
    "statusBarItem.remoteBackground": "#93395f",
    "statusBarItem.remoteForeground": "#e7e7e7",
    "titleBar.activeBackground": "#93395f",
